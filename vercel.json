{
  "version": 2,
  "builds": [
    {
      "src": "lobby.html",
      "use": "@vercel/static"
    },
    {
      "src": "room.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/lobby.html"
    },
    {
      "src": "/room",
      "dest": "/room.html"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/styles/(.*)",
      "dest": "/styles/$1"
    }
  ]
}
